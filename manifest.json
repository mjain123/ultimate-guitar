{
    "manifest_version": 2,
    
    "name": "Ultimate Guitar",

    "version": "1.0",

    "description": "Get chords and tabs",

    "icons": {
        "128": "icons/icon128.png",
        "64": "icons/icon64.png",
        "32": "icons/icon32.png",
        "16": "icons/icon16.png"
    },

    "browser_action": {
        "default_icon": "icons/icon16.png"
    },

    "background": {
        "scripts": ["jquery-3.5.1.min.js", "eventPage.js"],
        "persistent": false
    },

    "permissions": [
        "activeTab"
    ]
}